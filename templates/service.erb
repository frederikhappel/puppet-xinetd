# This file is managed by Puppet. DO NOT EDIT
service <%= @name %>
{
  disable         = <%= @disabled ? 'yes' : 'no' %>
  socket_type     = <%= @socket_type %>
  protocol        = <%= @protocol %>
  wait            = <%= @mywait ? 'yes' : 'no' %>
  user            = <%= @user %>
  group           = <%= @group %>
  server          = <%= @server %>
<% if !@port.nil? -%>
  type            = UNLISTED
  port            = <%= @port %>
<% end -%>
<% if !@server_args.nil? -%>
  server_args     = <%= @server_args %>
<% end -%>
<% if !@per_source.nil? -%>
  per_source      = <%= @per_source %>
<% end -%>
<% if !@cps.nil? -%>
  cps             = <%= @cps %>
<% end -%>
<% if !@flags.nil? -%>
  flags           = <%= @flags %>
<% end -%>
<% if !@log_on_failure.nil? -%>
  log_on_failure  = <%= @log_on_failure %>
<% end -%>
<% if !@log_on_success.nil? -%>
  log_on_success  = <%= @log_on_success %>
<% end -%>
}
